<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DirectoryHeader">
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="ContactListSeparatorTextViewStyle">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/people_app_theme_color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/list_section_divider_holo_custom</item>
        <item name="android:paddingLeft">8.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/list_section_divider_min_height</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:paddingStart">8.0dip</item>
    </style>
    <style name="ListViewStyle" parent="@android:style/Widget.Holo.Light.ListView">
        <item name="android:overScrollMode">always</item>
    </style>
    <style name="ContactListFilterTheme" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:listViewStyle">@style/ListViewStyle</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="CustomContactListFilterView" parent="@style/ContactListFilterTheme">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="BackgroundOnlyTheme" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowNoDisplay">true</item>
    </style>
    <style name="WindowAnimStyle">
        <item name="android:windowEnterAnimation">@android:anim/slide_in_left</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>
    <style name="DialtactsTheme" parent="@android:style/Theme.Holo.Light">
        <item name="android:textColorSecondary">@color/dialtacts_secondary_text_color</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:listViewStyle">@style/ListViewStyle</item>
        <item name="android:actionBarStyle">@style/DialtactsActionBarStyle</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:actionBarTabStyle">@style/DialtactsActionBarTabStyle</item>
        <item name="android:actionBarTabBarStyle">@style/DialtactsActionBarTabBarStyle</item>
        <item name="list_section_header_height">32.0dip</item>
        <item name="activated_background">@drawable/list_item_activated_background</item>
        <item name="section_header_background">@drawable/list_title_holo</item>
        <item name="list_item_divider">?android:listDivider</item>
        <item name="list_item_padding_top">0.0dip</item>
        <item name="list_item_padding_right">0.0dip</item>
        <item name="list_item_padding_bottom">0.0dip</item>
        <item name="list_item_padding_left">0.0dip</item>
        <item name="list_item_gap_between_image_and_text">8.0dip</item>
        <item name="list_item_gap_between_label_and_data">5.0dip</item>
        <item name="list_item_presence_icon_margin">4.0dip</item>
        <item name="list_item_presence_icon_size">16.0dip</item>
        <item name="list_item_photo_size">@dimen/contact_browser_list_item_photo_size</item>
        <item name="list_item_profile_photo_size">70.0dip</item>
        <item name="list_item_prefix_highlight_color">@color/prefix_hightlight_color</item>
        <item name="list_item_header_text_indent">8.0dip</item>
        <item name="list_item_header_text_color">@color/people_app_theme_color</item>
        <item name="list_item_header_text_size">14.0sp</item>
        <item name="list_item_header_height">24.0dip</item>
        <item name="list_item_header_underline_height">1.0dip</item>
        <item name="list_item_header_underline_color">@color/people_app_theme_color</item>
        <item name="list_item_text_indent">@dimen/contact_browser_list_item_text_indent</item>
        <item name="list_item_data_width_weight">5</item>
        <item name="list_item_label_width_weight">3</item>
        <item name="list_item_account_icon_size">@dimen/contact_browser_list_account_icon_size</item>
        <item name="list_item_call_btn_size">@dimen/contact_browser_list_item_call_btn_size</item>
        <item name="contact_browser_list_padding_left">16.0dip</item>
        <item name="contact_browser_list_padding_right">0.0dip</item>
        <item name="contact_browser_background">@android:color/transparent</item>
        <item name="favorites_padding_bottom">?android:actionBarSize</item>
        <item name="call_log_primary_text_color">#ffffffff</item>
        <item name="call_log_primary_background_color">#ff000000</item>
        <item name="call_log_secondary_text_color">#ff888888</item>
        <item name="call_log_secondary_background_color">#ff333333</item>
        <item name="call_log_header_color">#ff33b5e5</item>
        <item name="call_log_voicemail_status_height">48.0dip</item>
        <item name="call_log_voicemail_status_background_color">#ff262626</item>
        <item name="call_log_voicemail_status_text_color">#ff888888</item>
        <item name="call_log_voicemail_status_action_text_color">#ff33b5e5</item>
    </style>
    <style name="CallDetailActivityTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:listViewStyle">@style/ListViewStyle</item>
        <item name="android:gravity">top</item>
        <item name="call_log_primary_text_color">#ffffffff</item>
        <item name="call_log_primary_background_color">#ff000000</item>
        <item name="call_log_secondary_text_color">#ffffffff</item>
        <item name="call_log_secondary_background_color">#ff333333</item>
        <item name="call_log_header_color">#ff33b5e5</item>
        <item name="call_log_voicemail_status_height">48.0dip</item>
        <item name="call_log_voicemail_status_background_color">#ff262626</item>
        <item name="call_log_voicemail_status_text_color">#ff888888</item>
        <item name="call_log_voicemail_status_action_text_color">#ff33b5e5</item>
    </style>
    <style name="Theme" />
    <style name="DialtactsDigitsTextAppearance">
        <item name="android:textSize">@dimen/dialpad_digits_text_size</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:cursorVisible">false</item>
        <item name="android:maxLines">1</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:editable">true</item>
        <item name="android:freezesText">true</item>
        <item name="android:layout_weight">0.0</item>
    </style>
    <style name="DialtactsDialpadButtonStyle">
        <item name="android:background">@drawable/dial_num_background</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:soundEffectsEnabled">false</item>
    </style>
    <style name="DialpadHorizontalSeparator">
        <item name="android:background">#66000000</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/dialpad_vertical_margin</item>
    </style>
    <style name="DialtactsActionBarStyle" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:displayOptions">none</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_opaque_dark_holo</item>
        <item name="android:backgroundSplit">@drawable/ic_tab_split_bg</item>
    </style>
    <style name="DialtactsActionBarTabBarStyle" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:divider">@drawable/ic_tab_divider</item>
        <item name="android:showDividers">middle</item>
        <item name="android:dividerPadding">0.0dip</item>
    </style>
    <style name="DialtactsActionBarTabStyle" parent="@android:style/Widget.Holo.ActionBar.TabView">
        <item name="android:background">@drawable/action_bar_tab_custom</item>
    </style>
    <style name="FragmentActionBarPadding">
        <item name="android:paddingBottom">?android:actionBarSize</item>
    </style>
    <style name="SpeedDialTitleStyle">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:textColor">@color/unset_title_color</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="CallMultiChoiceTheme" parent="@style/DialtactsTheme">
        <item name="android:windowActionBarOverlay">false</item>
    </style>
    <style name="call_log_tab_bottom">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@drawable/call_log_background</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:button">@null</item>
        <item name="android:maxLines">0</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="DialtactsDialpadButtonStyle2">
        <item name="android:background">@drawable/dial_num_background2</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:soundEffectsEnabled">false</item>
    </style>
</resources>
